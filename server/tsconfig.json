{
  "compilerOptions": {
    "target": "ES6",
    "outDir": "./build/",
    "module": "NodeNext",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "lib": ["ES6", "DOM", "ESNext.AsyncIterable"]
  }
}
